
plot1_func <- function(pow_data){
        ## Create the histogram
        hist(pow_data$Global_active_power, main="Global Active Power", xlab = "Global Active Power (kilowatts)", col="red")
        ## Save file and close device
        dev.copy(png, file = "plot1.png", width = 480, height = 480)
        dev.off()
        
}
